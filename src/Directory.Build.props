<Project>
    <Import Project="../build/version.props" />
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <LangVersion>8.0</LangVersion>
        <PackageIdBase>AspNetCore.EasyTesting</PackageIdBase>
        <PackageIconUrl>https://raw.githubusercontent.com/WDWWW/aspnetcore-easy-testing/master/asset/wd3w.png</PackageIconUrl>
        <Authors>Jinseoung Lee</Authors>
        <PackageTags>aspnetcore;aspnet;test;integration;integration-testing;e2e;</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/WDWWW/aspnetcore-easy-testing</PackageProjectUrl>
    </PropertyGroup>
    
    <PropertyGroup>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>
</Project>